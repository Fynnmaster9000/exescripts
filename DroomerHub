			local part = Instance.new("Part")
			part.Size = Vector3.new(1, 1, 1)
			part.Position = Vector3.new(math.random(-50, 50), 100, math.random(-50, 50))
			part.Anchored = false
			part.Parent = workspace
		end
	end)
	
end
coroutine.wrap(DGTZKP_fake_script)()
local function HDOH_fake_script() -- EditorFrame.Execute 
	local script = Instance.new('LocalScript', EditorFrame)

	local ExploitAPI = {} -- Replace this with your exploit API functions
	local ExecuteButton = script.Parent.Execute -- The Execute button
	local TextBox = script.Parent.TextBox -- The script input box
	local LevelLabel = script.Parent.LevelLabel -- The label showing API Level
	
	-- Function to execute the script
	local function ExecuteScript()
		local scriptToExecute = TextBox.Text
	
		if scriptToExecute and scriptToExecute ~= "" then
			local success, err = pcall(function()
				-- Try using the Exploit API execution function
				if ExploitAPI.Execute then
					ExploitAPI.Execute(scriptToExecute) 
				elseif loadstring then
					-- Fallback method using loadstring if supported
					local compiledScript = loadstring(scriptToExecute)
					if compiledScript then
						compiledScript()
					else
						error("Failed to compile script.")
					end
				else
					error("No valid execution method found.")
				end
			end)
	
			-- Handle execution errors
			if not success then
				warn("Execution failed: " .. tostring(err))
			end
	
			-- Update API Level Label
			local level = ExploitAPI.GetLevel and ExploitAPI.GetLevel() or "Unknown"
			LevelLabel.Text = "API Level: " .. level
		else
			warn("No script entered.")
		end
	end
	
	-- Bind function to execute button
	ExecuteButton.MouseButton1Click:Connect(ExecuteScript)
	
end
coroutine.wrap(HDOH_fake_script)()
local function QWTWNB_fake_script() -- Close_2.LocalScript 
	local script = Instance.new('LocalScript', Close_2)

	local button = script.Parent -- The GUI button
	
	button.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer
		local playerGui = player:FindFirstChildOfClass("PlayerGui")
	
		if playerGui then
			local screenGui = playerGui:FindFirstChild("DroomerHub")
	
			if screenGui then
				local frame = screenGui:FindFirstChild("Frame")
	
				if frame then
					frame.Visible = false -- Hides the frame (closes it)
					print("Frame found and closed!")
				else
					print("Frame not found in ScreenGui")
				end
			else
				print("ScreenGui not found in PlayerGui")
			end
		else
			print("PlayerGui not found")
		end
	end)
	
end
