local function AVJTI_fake_script() -- Settings.LocalScript 
	local script = Instance.new('LocalScript', Settings)

	local button = script.Parent -- The GUI button
	local editorFrame = script.Parent.Parent:FindFirstChild("EditorFrame")
	local cloudFrame = script.Parent.Parent:FindFirstChild("CloudFrame")
	local setFrame = script.Parent.Parent:FindFirstChild("SetFrame")
	
	if editorFrame and cloudFrame and setFrame then
		button.MouseButton1Click:Connect(function()
			editorFrame.Visible = false
			cloudFrame.Visible = false
			setFrame.Visible = true
		end)
	end
	
end
