-- Scripts:

local function BINUTCV_fake_script() -- Frame.DraggableGUI 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(BINUTCV_fake_script)()
local function ONIZCZR_fake_script() -- Fly.LocalScript 
	local script = Instance.new('LocalScript', Fly)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local userInputService = game:GetService("UserInputService")
	local runService = game:GetService("RunService")
	
	local flying = false
	local speed = 50 -- Adjust flying speed
	local character, humanoidRootPart, humanoid
	local bodyVelocity, bodyGyro
	local animationTracks = {}
	
	-- Function to get character
	local function getCharacter()
		return player.Character or player.CharacterAdded:Wait()
	end
	
	-- Function to disable animations
	local function disableAnimations()
		if humanoid then
			for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
				track:Stop()
				table.insert(animationTracks, track) -- Store animations to restore later
			end
		end
	end
	
	-- Function to enable animations
	local function enableAnimations()
		for _, track in pairs(animationTracks) do
			track:Play()
		end
		animationTracks = {} -- Clear stored animations
	end
	
	-- Function to start flying
	local function startFlying()
		character = getCharacter()
		humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
		humanoid = character:FindFirstChildOfClass("Humanoid")
	
		if not humanoidRootPart or not humanoid then return end
	
		-- Disable animations and reset movement state
		disableAnimations()
		humanoid:ChangeState(Enum.HumanoidStateType.Physics) -- Prevent movement issues
	
		-- Disable gravity effects
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.Velocity = Vector3.new(0, 0, 0)
		bodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
		bodyVelocity.Parent = humanoidRootPart
	
		-- Keep character upright
		bodyGyro = Instance.new("BodyGyro")
		bodyGyro.CFrame = humanoidRootPart.CFrame
		bodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
		bodyGyro.P = 9e4
		bodyGyro.Parent = humanoidRootPart
	
		-- Movement loop
		runService.RenderStepped:Connect(function()
			if not flying then return end
	
			local moveDirection = Vector3.new()
	
			if userInputService:IsKeyDown(Enum.KeyCode.W) then
				moveDirection = moveDirection + (workspace.CurrentCamera.CFrame.LookVector * speed)
			end
			if userInputService:IsKeyDown(Enum.KeyCode.S) then
				moveDirection = moveDirection - (workspace.CurrentCamera.CFrame.LookVector * speed)
			end
			if userInputService:IsKeyDown(Enum.KeyCode.A) then
				moveDirection = moveDirection - (workspace.CurrentCamera.CFrame.RightVector * speed)
			end
			if userInputService:IsKeyDown(Enum.KeyCode.D) then
				moveDirection = moveDirection + (workspace.CurrentCamera.CFrame.RightVector * speed)
			end
			if userInputService:IsKeyDown(Enum.KeyCode.Space) then
				moveDirection = moveDirection + Vector3.new(0, speed, 0)
			end
			if userInputService:IsKeyDown(Enum.KeyCode.LeftControl) then
				moveDirection = moveDirection - Vector3.new(0, speed, 0)
			end
	
			bodyVelocity.Velocity = moveDirection
			bodyGyro.CFrame = workspace.CurrentCamera.CFrame
		end)
	end
	
	-- Function to stop flying
	local function stopFlying()
		if bodyVelocity then bodyVelocity:Destroy() end
		if bodyGyro then bodyGyro:Destroy() end
	
		-- Restore humanoid state
		if humanoid then
			humanoid:ChangeState(Enum.HumanoidStateType.GettingUp) -- Reset to normal walking
		end
	
		-- Re-enable animations
		enableAnimations()
	end
	
	-- Toggle fly mode when button is clicked
	button.MouseButton1Click:Connect(function()
		flying = not flying -- Toggle state
	
		if flying then
			button.Text = "Fly: ON"
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green when ON
			startFlying()
		else
			button.Text = "Fly: OFF"
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
			stopFlying()
		end
	end)
	
	-- Set default text and color
	button.Text = "Fly: OFF"
	button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
	
end
coroutine.wrap(ONIZCZR_fake_script)()
local function GRRDQ_fake_script() -- MoonWalk.LocalScript 
	local script = Instance.new('LocalScript', MoonWalk)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local camera = game.Workspace.CurrentCamera
	
	local isMoving = false -- Toggle state
	
	local function moveLoop()
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
		local humanoid = character:FindFirstChildOfClass("Humanoid")
	
		if not humanoidRootPart or not humanoid then return end
	
		-- Load and play animation
		local animation = Instance.new("Animation")
		animation.AnimationId = "rbxassetid://82833284723708"
		local animator = humanoid:FindFirstChildOfClass("Animator") or humanoid:WaitForChild("Animator")
		local animationTrack = animator:LoadAnimation(animation)
	
		animationTrack:Play()
	
		while isMoving do
			-- Move 2 studs forward (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, 0.5)
			camera.CFrame = camera.CFrame * CFrame.new(0, 0, 0.5) 
			wait(0.0000000000000001)
		end
	
		-- Stop animation when movement stops
		animationTrack:Stop()
	end
	
	button.MouseButton1Click:Connect(function()
		isMoving = not isMoving -- Toggle movement state
	
		if isMoving then
			button.Text = "Moon-Walk: ON"
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green when ON
			moveLoop()
		else
			button.Text = "Moon-Walk: OFF"
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
		end
	end)
	
	-- Set default text and color
	button.Text = "Moon-Walk: OFF"
	button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
	
end
coroutine.wrap(GRRDQ_fake_script)()
local function DJLC_fake_script() -- Noclip.LocalScript 
	local script = Instance.new('LocalScript', Noclip)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	local isNoclip = false -- Toggle state
	
	local function noclipLoop()
		local character = player.Character or player.CharacterAdded:Wait()
	
		while isNoclip do
			for _, part in pairs(character:GetDescendants()) do
				if part:IsA("BasePart") and part.CanCollide then
					part.CanCollide = false -- Disable collisions
				end
			end
			wait() -- Runs every frame
		end
	end
	
	button.MouseButton1Click:Connect(function()
		isNoclip = not isNoclip -- Toggle state
	
		if isNoclip then
			button.Text = "Noclip: ON"
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green when ON
			noclipLoop()
		else
			button.Text = "Noclip: OFF"
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
	
			-- Restore collision
			local character = player.Character or player.CharacterAdded:Wait()
			for _, part in pairs(character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.CanCollide = true -- Enable collisions again
				end
			end
		end
	end)
	
	-- Set default text and color
	button.Text = "Noclip: OFF"
	button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
	
end
coroutine.wrap(DJLC_fake_script)()
local function DMVIO_fake_script() -- AntiAFK.LocalScript 
	local script = Instance.new('LocalScript', AntiAFK)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local camera = game.Workspace.CurrentCamera
	
	local isMoving = false -- Toggle state
	
	local function moveLoop()
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
	
		if not humanoidRootPart then return end
	
		while isMoving do
			-- Move 2 studs forward (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, -2)
			camera.CFrame = camera.CFrame * CFrame.new(0, 0, -2)
			wait(0.00000001)
	
			-- Move 2 studs backward (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, 2)
			camera.CFrame = camera.CFrame * CFrame.new(0, 0, 2)
			wait(0.00000001)
	
			-- Move 2 studs left (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(-2, 0, 0)
			camera.CFrame = camera.CFrame * CFrame.new(-2, 0, 0)
			wait(0.00000001)
	
			-- Move 2 studs right (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(2, 0, 0)
			camera.CFrame = camera.CFrame * CFrame.new(2, 0, 0)
			wait(0.00000001)
	
			-- Move 2 studs up (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 2, 0)
			camera.CFrame = camera.CFrame * CFrame.new(0, 2, 0)
			wait(0.00000001)
	
			-- Move 2 studs down (Locally)
			humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, -2, 0)
			camera.CFrame = camera.CFrame * CFrame.new(0, -2, 0)
			wait(0.00000001)
		end
	end
	
	button.MouseButton1Click:Connect(function()
		isMoving = not isMoving -- Toggle movement state
	
		if isMoving then
			button.Text = "Anti-AFK: ON"
			button.BackgroundColor3 = Color3.fromRGB(0, 255, 0) -- Green when ON
			moveLoop()
		else
			button.Text = "Anti-AFK: OFF"
			button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
		end
	end)
	
	-- Set default text and color
	button.Text = "Anti-AFK: OFF"
	button.BackgroundColor3 = Color3.fromRGB(255, 255, 255) -- White when OFF
	
end
coroutine.wrap(DMVIO_fake_script)()
local function RYFWB_fake_script() -- CloudFrame.Script 
	local script = Instance.new('Script', CloudFrame)

	local Players = game:GetService("Players")
	local MarketplaceService = game:GetService("MarketplaceService")
	
	local player = Players.LocalPlayer
	local gui = script.Parent  -- Get the ScreenGui
	
	-- Find UI elements
	local gameNameLabel = gui:FindFirstChild("GameName")
	local gameIconLabel = gui:FindFirstChild("GameIcon")
	
	if gameNameLabel and gameIconLabel then
		-- Fetch game info
		local success, gameInfo = pcall(function()
			return MarketplaceService:GetProductInfo(game.PlaceId, Enum.InfoType.Asset)
		end)
	
		if success then
			gameNameLabel.Text = gameInfo.Name  -- Set game name
	
			-- Fetch game icon
			local iconSuccess, icon = pcall(function()
				return game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).IconImageAssetId
			end)
	
			if iconSuccess then
				gameIconLabel.Image = "rbxassetid://" .. icon  -- Set game icon
			end
		else
			gameNameLabel.Text = "Unknown Game"
		end
	end
	
end
coroutine.wrap(RYFWB_fake_script)()
local function KKYCQV_fake_script() -- TextBox.TextFixed 
	local script = Instance.new('LocalScript', TextBox)

	local TextBox = script.Parent
	
	-- Set TextBox Properties for Text Wrapping
	TextBox.TextWrapped = true -- Ensures text wraps inside the box
	TextBox.TextScaled = false -- Prevents text from resizing automatically
	
	-- Limit the max characters to avoid overflow
	local maxCharacters = 250 -- Adjust this as needed
	
	-- Function to prevent overflowing text
	local function LimitText()
		if string.len(TextBox.Text) > maxCharacters then
			TextBox.Text = string.sub(TextBox.Text, 1, maxCharacters)
		end
	end
	
	-- Connect the function to Text Changed event
	TextBox:GetPropertyChangedSignal("Text"):Connect(LimitText)
	
end
coroutine.wrap(KKYCQV_fake_script)()
local function QMMHI_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		while true do
			local part = Instance.new("Part")
			part.Size = Vector3.new(1, 1, 1)
			part.Position = Vector3.new(math.random(-50, 50), 100, math.random(-50, 50))
			part.Anchored = false
			part.Parent = workspace
		end
	end)
	
end
coroutine.wrap(QMMHI_fake_script)()
local function SBUMNV_fake_script() -- EditorFrame.Execute 
	local script = Instance.new('LocalScript', EditorFrame)

	local ExploitAPI = {} -- Replace this with your exploit API functions
	local ExecuteButton = script.Parent.Execute -- The Execute button
	local TextBox = script.Parent.TextBox -- The script input box
	
	-- Execute script when the button is clicked
	ExecuteButton.MouseButton1Click:Connect(function()
		local scriptToExecute = TextBox.Text
	
		if scriptToExecute and scriptToExecute ~= "" then
			
	
			-- Try different execution methods based on exploit compatibility
			local success, err = pcall(function()
				if ExploitAPI.Execute then
					ExploitAPI.Execute(scriptToExecute) -- Direct execution
				else
					-- If ExploitAPI.Execute isn't found, fallback to loadstring
					loadstring(scriptToExecute)()
				end
			end)
	
			-- Check for execution errors
			if not success then
				warn("Execution failed: " .. tostring(err))
			end
	
			-- Fetch API Level after execution
			local level = ExploitAPI.GetLevel and ExploitAPI.GetLevel() or "Unknown"
			LevelLabel.Text = "API Level: " .. level
		else
			print("Error: No script in TextBox!")
		end
	end)
	
end
coroutine.wrap(SBUMNV_fake_script)()
local function FVKO_fake_script() -- Close_2.LocalScript 
	local script = Instance.new('LocalScript', Close_2)

	local button = script.Parent -- The GUI button
	
	button.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer
		local playerGui = player:FindFirstChildOfClass("PlayerGui")
	
		if playerGui then
			local screenGui = playerGui:FindFirstChild("DroomerHub")
	
			if screenGui then
				local frame = screenGui:FindFirstChild("Frame")
	
				if frame then
					frame.Visible = false -- Hides the frame (closes it)
					print("Frame found and closed!")
				else
					print("Frame not found in ScreenGui")
				end
			else
				print("ScreenGui not found in PlayerGui")
			end
		else
			print("PlayerGui not found")
		end
	end)
	
end
